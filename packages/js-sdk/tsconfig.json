{
  "compilerOptions": {
    "outDir": "dist/js",
    "noImplicitAny": false,
    "strictNullChecks": true,
    "module": "commonjs",
    "target": "es5",
    "allowJs": false,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "lib": [
      "es2015"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}